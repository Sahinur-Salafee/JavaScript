var Person = function (name, age, job) {
    this.name = name;
    this.age = age;
    this.job = job;
    this.dateOfBirth = function () {
        console.log(this.name + ' was born in ' + (2023 - this.age));
    }
}

var salafee = new Person('Salafee', 27, 'Software Engineer');
console.log(salafee);
console.log(salafee.name);
console.log(salafee.age);
console.log(salafee.job);
console.log(salafee.dateOfBirth());